{
  "name": "@alist2strm/server",
  "version": "1.0.0",
  "description": "AList2Strm server application",
  "type": "module",
  "main": "dist/index.js",
  "packageManager": "pnpm@10.10.0",
  "pnpm": {
    "onlyBuiltDependencies": ["sqlite3"]
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch --tsconfig tsconfig.json src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "pnpm run build && cross-env NODE_ENV=production tsx --tsconfig tsconfig.json dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "log4js": "^6.9.1",
    "node-cron": "^4.0.5",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.47",
    "@types/node-cron": "^3.0.11",
    "@types/validator": "^13.11.9",
    "eslint": "^8.57.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3"
  }
}