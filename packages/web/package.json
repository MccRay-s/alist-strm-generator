{
  "name": "@alist-strm/web",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@iconify/json": "^2.2.337",
    "@types/node": "^20.11.19",
    "@unocss/eslint-plugin": "^0.58.5",
    "@unocss/preset-attributify": "^0.58.5",
    "@unocss/preset-icons": "^0.58.5",
    "@unocss/preset-web-fonts": "^0.58.5",
    "@unocss/preset-wind": "^0.58.5",
    "@unocss/reset": "^0.58.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue-macros/volar": "^0.17.4",
    "@vue/test-utils": "^2.4.4",
    "@vueuse/core": "^10.7.2",
    "cron-validate": "^1.5.2",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "naive-ui": "^2.41.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.3",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.10",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.1.3",
    "vitest": "^1.3.0",
    "vue-tsc": "^1.8.27"
  },
  "resolutions": {
    "unplugin": "catalog:build",
    "vite": "catalog:build"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}